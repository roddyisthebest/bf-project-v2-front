{
  "name": "frontend",
  "version": "1.0.2",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "codepush:android": "appcenter codepush release-react -a seongyeon980123-gmail.com/bf-church-v2-android -d Staging --sourcemap-output --output-dir ./build -m -t 1.0",
    "codepush:ios": "appcenter codepush release-react -a seongyeon980123-gmail.com/bf-church-v2-ios -d Staging --sourcemap-output --output-dir ./build -m -t 1.0",
    "bundle:android": "react-native bundle --assets-dest build/CodePush --bundle-output build/CodePush/index.android.bundle --dev false --entry-file index.js --platform android --sourcemap-output build/CodePush/index.android.bundle.map",
    "bundle:ios": "react-native bundle --assets-dest build/CodePush --bundle-output build/CodePush/main.jsbundle --dev false --entry-file index.js --platform ios --sourcemap-output build/CodePush/main.jsbundle.map",
    "appcenter-android-key": "appcenter codepush deployment list -a seongyeon980123-gmail.com/bf-church-v2-android -k",
    "appcenter-ios-key": "appcenter codepush deployment list -a seongyeon980123-gmail.com/bf-church-v2-ios -k"
  },
  "dependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.19.0",
    "@react-native-community/push-notification-ios": "^1.10.1",
    "@react-native-community/viewpager": "^5.0.11",
    "@react-native-firebase/analytics": "^15.4.0",
    "@react-native-firebase/app": "^15.4.0",
    "@react-native-firebase/messaging": "^15.4.0",
    "@react-navigation/bottom-tabs": "^6.3.2",
    "@react-navigation/material-top-tabs": "^6.2.2",
    "@react-navigation/native-stack": "^6.7.0",
    "@reduxjs/toolkit": "^1.8.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react-native": "^11.0.0",
    "@types/react-native-vector-icons": "^6.4.11",
    "appcenter": "4.4.5",
    "appcenter-analytics": "4.4.5",
    "appcenter-crashes": "4.4.5",
    "axios": "^0.27.2",
    "babel-plugin-import": "^1.13.5",
    "form-data": "^4.0.0",
    "heic2any": "^0.0.3",
    "moment": "^2.29.4",
    "node-fetch": "^3.2.10",
    "react": "17.0.2",
    "react-dom": "^17.0.2",
    "react-native": "0.68.2",
    "react-native-code-push": "^7.0.5",
    "react-native-config": "^1.4.6",
    "react-native-drop-shadow": "^0.0.5",
    "react-native-encrypted-storage": "^4.0.2",
    "react-native-fast-image": "^8.5.11",
    "react-native-image-modal": "^2.0.4",
    "react-native-image-picker": "^4.8.5",
    "react-native-image-zoom-viewer": "^3.0.1",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-push-notification": "^8.1.1",
    "react-native-safe-area-context": "^4.3.1",
    "react-native-screens": "^3.15.0",
    "react-native-tab-view": "^3.1.1",
    "react-native-vector-icons": "^9.2.0",
    "react-native-webview": "^11.23.0",
    "react-redux": "^8.0.2",
    "socket.io-client": "^4.5.1",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/jest-native": "^4.0.11",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.67.3",
    "@types/react-native-push-notification": "^8.1.1",
    "@types/react-test-renderer": "^17.0.1",
    "@types/styled-components": "^5.1.25",
    "@types/styled-components-react-native": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.67.0",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "clearMocks": true,
    "collectCoverage": false,
    "coverageDirectory": "coverage",
    "coverageProvider": "v8",
    "moduleFileExtensions": [
      "js",
      "mjs",
      "cjs",
      "jsx",
      "ts",
      "tsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "testEnvironment": "jest-environment-node",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?@?react-native)"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ]
  }
}
